{
  "arrowParens": "always",
  "bracketSameLine": true,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxSingleQuote": false,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "printWidth": 220,
  "endOfLine": "auto",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": ["**/*.html"],
      "options": {
        "singleQuote": false,
        "bracketSameLine": true
      }
    }
  ]
}
